<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.wrq.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserListServlet</servlet-name>
        <servlet-class>com.wrq.servlet.UserListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VipListServlet</servlet-name>
        <servlet-class>com.wrq.servlet.VipListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAddServlet</servlet-name>
        <servlet-class>com.wrq.servlet.UserAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PasswordMdiServlet</servlet-name>
        <servlet-class>com.wrq.servlet.PasswordMdiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserMdiQueryServlet</servlet-name>
        <servlet-class>com.wrq.servlet.UserMdiQueryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserMdiServlet</servlet-name>
        <servlet-class>com.wrq.servlet.UserMdiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserDelServlet</servlet-name>
        <servlet-class>com.wrq.servlet.UserDelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VipAddServlet</servlet-name>
        <servlet-class>com.wrq.servlet.VipAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VipDelServlet</servlet-name>
        <servlet-class>com.wrq.servlet.VipDelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VipMdiQueryServlet</servlet-name>
        <servlet-class>com.wrq.servlet.VipMdiQueryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VipMdiServlet</servlet-name>
        <servlet-class>com.wrq.servlet.VipMdiServlet</servlet-class>
    </servlet>
    <filter>
        <filter-name>EncodingFilter</filter-name>
        <filter-class>com.wrq.filter.EncodingFilter</filter-class>
    </filter>
    <filter>
        <filter-name>SessionFilter</filter-name>
        <filter-class>com.wrq.filter.SessionFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>SessionFilter</filter-name>
        <url-pattern>*.jsp</url-pattern>
        <url-pattern>*.do</url-pattern>
    </filter-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserListServlet</servlet-name>
        <url-pattern>/UserListServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VipListServlet</servlet-name>
        <url-pattern>/VipListServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAddServlet</servlet-name>
        <url-pattern>/UserAddServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PasswordMdiServlet</servlet-name>
        <url-pattern>/PasswordMdiServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserMdiQueryServlet</servlet-name>
        <url-pattern>/UserMdiQueryServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserMdiServlet</servlet-name>
        <url-pattern>/UserMdiServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserDelServlet</servlet-name>
        <url-pattern>/UserDelServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VipAddServlet</servlet-name>
        <url-pattern>/VipAddServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VipDelServlet</servlet-name>
        <url-pattern>/VipDelServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VipMdiQueryServlet</servlet-name>
        <url-pattern>/VipMdiQueryServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VipMdiServlet</servlet-name>
        <url-pattern>/VipMdiServlet.do</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
